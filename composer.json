{
    "name": "mekras/odata-client",
    "description": "OData client-side library",
    "type": "library",
    "license": "MIT",
    "keywords": ["OData"],
    "authors": [
        {
            "name": "Михаил Красильников",
            "email": "m.krasilnikov@yandex.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Mekras\\OData\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mekras\\OData\\Client\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "mekras/interfaces": "^3.0",
        "mekras/atompub": "~0.1.1",
        "php-http/client-implementation": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.3.1",
        "php-http/curl-client": "^1.4.2",
        "php-http/message": "^1.2",
        "phpunit/phpunit": "^5.0.9",
        "satooshi/php-coveralls": "dev-master"
    }
}